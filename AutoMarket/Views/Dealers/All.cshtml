@model  ICollection<string>
@{ 
    ViewData["Title"] = "Търсене по Дилъри";
}
<h3>Тук можете да намерите всички обяви на избраният от вас Дилър</h3>
<form method="get">
    <div class="row">
        <div class="col-md-6 col-sm-12 col-xl-9">
            <div>
                <label for="Dealers"> Dealers</label>
                <select class="form-group" name="Dealers">
                    <option value="">Всички</option>
                    @foreach (var dealer in Model)
                    {
                        <option value="@dealer">@dealer</option>
                    }
                </select>
            </div>
            <div class="form-group">
                <input type="submit" class="btn btn-primary" value="Търси" />
            </div>
        </div>
    </div>
</form>