@model ListAllVehicleViewModel
@{
    ViewData["Title"] = "Активни обяви за автомобили";
}
<h1 class="text-center mt-2">Обяви за автомобили</h1>
<div class="wrapper">
    <a class="btn btn-outline-info" href="/Search/All"><i class="fas fa-search"></i> Търсене</a>
</div>
@if (!Model.Offers.Any())
{
    <p>Все още няма обяви в секция 'Автомобили'</p>
}
<partial name="_VehicleAllPartial" model="@Model" />
<hr />
<partial name="_PagingPartial" model="@Model" />

